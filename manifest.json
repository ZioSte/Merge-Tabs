{
  "manifest_version": 3,
  "name": "Dividi Schede per Dominio",
  "version": "1.0",
  "description": "Divide le schede aperte in finestre separate per dominio.",
  "permissions": ["tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
}